            {
              "id": "CVE-2024-6345",
              "name": "OsPackageVulnerability",
              "shortDescription": {
                "text": "CVE-2024-6345: Improper Control of Generation of Code ('Code Injection')"
              },
              "helpUri": "https://scout.docker.com/v/CVE-2024-6345?s=github&n=setuptools&t=pypi&vr=%3C70.0.0",
